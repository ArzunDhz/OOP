#include <iostream>

using namespace std;
 
 int fact(int n);
 
 int main(){
 int n ;
  cout<<"enter any number";
  cin>>n;
 
  cout<<fact(n);
 }



 int fact(int n ){
 if(n==0||n==1){
   return 1;

   }
     else{
     return (n*fact(n-1));
 }

 }
________________________________________11_____________________
#include <iostream>

using namespace std;
 
 int fubu(int n);
 
 int main(){
 int n ;
  cout<<"enter the no of series";
  cin>>n;
 for(int i =0;i<n;i++){
 cout<<fubu(i);
 }
  
 }



 int fubu(int n ){
 if(n==0||n==1){
   return n;

   }
     else{
     return (fubu(n-1)+fubu(n-2));
 }

 }
