#include <iostream>
using namespace std;

int hms_to_secs(int h ,int m ,int s ){
return (h*60*60+m*60+s);
}
int main(){
cout<<"enter hr min sec";
int a,b,c;
cin>>a>>b>>c;
long sec;
sec = hms_to_secs(a,b,c);
cout<<sec;

}
