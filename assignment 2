#include <iostream>

using namespace std;

inline int area(int a){
  return 3.14*a*a;
}

int main(){
  int r;
cout<<"value of radious";
cin>>r;
cout<<"areais "<<area(r);


}
____________________________________________4_______________________________________

#include <iostream>

using namespace std;
float area(int a){
  return 3.14*a*a;
}
int area(int a,int b, int c){
  int s = (a+b+c)/2;
  return (s*(s-a)*(s-b)*(s-c));
}
 int area(int a ,int b){
  return a*b;
}
int main(){
  int a,b,c;
  cout<<"input a , b ,c";
  cin>>a>>b>>c;
cout<<"circle area ="<<area(a)<<endl;
cout<<"reatranglearea ="<<area(a,b)<<endl;
cout<<"tringle area ="<<area(a,b,c)<<endl;
}
