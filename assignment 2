____________________________________________7_______________________________________

#include <iostream>

using namespace std;
float area(int a){
  return 3.14*a*a;
}
int area(int a,int b, int c){
  int s = (a+b+c)/2;
  return (s*(s-a)*(s-b)*(s-c));
}
 int area(int a ,int b){
  return a*b;
}
int main(){
  int a,b,c;
  cout<<"input a , b ,c";
  cin>>a>>b>>c;
cout<<"circle area ="<<area(a)<<endl;
cout<<"reatranglearea ="<<area(a,b)<<endl;
cout<<"tringle area ="<<area(a,b,c)<<endl;
}
_________________________________________8_____________________________________


#include <iostream>
int display();
using namespace std;
struct date{
  int m ,d ,y ;

};

int display(date dt){
  
  cout<<"the date is "<<dt.m<<"/"<<dt.d<<"/"<<dt.y;
}



int main()
{
    cout<<"Enter the date in the format mm/dd/yyyy: "<<endl;
    date dt;
    cin>>dt.m>>dt.d>>dt.y;
    display(dt);

    return 0;
}

_____________________________________________________________9______________________________________
#include <iostream>

using namespace std;
struct height{
  int f , i;

};

int display(height h){

  cout<<"the date is "<<h.f<<"'"<<h.i<<"";
}


int main()
{
    cout<<"Enter the height  in the format feet inch "<<endl;
    height h;
    cin>>h.f>>h.i;
    if(h.i>=12){
        h.f = h.f + h.i/12;
        h.i= h.i%12;
    }
    display(h);

    return 0;
}
