#include <iostream>

using namespace std;
int &greater();
int x,y;

int main(){
int s = 1000 ;
cout<<"enter 2 no";
cin>>x>>y;
greater()=s;
if(x==0){
    cout<<"smaller temp"<<x<<"greater value"<<y;
}
if(y==0){
    cout<<"smaller value"<<y<<"greater value"<<x;
}
else{
    cout<<"both values ate equal";

}
return 0;
}

int &greater(){
if(x>y){
    return y ;
}
else
    return x;



}



