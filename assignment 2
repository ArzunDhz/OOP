#include <iostream>
using namespace std;
int zeroSmaller(int a , int b ){
if(a>b){
    return b;
}
else{
    return a ;
}
}

int main(){
int a,b ;
cout<<"enter any number";
cin>>a>>b;
cout<<"smaller no is "<<zeroSmaller (a,b);

}

