#include <iostream>

using namespace std;


int temp(int &a, int &b){
 if(a > b ){
   return a;
 }
 else{
   return b ; 
 }
}

int main(){
int a, b;
int s = 100;
cout<<"input 2 value of input";
cin>>a>>b;
temp(a,b)=s;
if(a==100){
cout<<"tepm of a"<<a<<"temp of b"<<b;
}
if(b==100){
cout<<"tepm of a"<<a<<"temp of b"<<b;
}
else{
  cout<<"both are equal";
}



}



