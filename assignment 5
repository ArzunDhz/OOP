#include<iostream>
#include<string.h>
using namespace std;

class Account{
private:
    char name[10];
    int no , balance;
public:
    int getdata(char a[10] , int b ,int c );
    int display();
};

int Account::getdata(char a[10] , int b ,int c ){
strcpy(name,a);
no=b;
balance=c;
}

int Account::display(){
cout<<"\n";
cout<<"  Name =  "<<name<<"  Acc_no =  "<<no<<"  balance = "<<balance<<"$"<<endl;
}


int main(){
Account info[5];
char a[10];
int b,c;
int n ;
cout<<"no of customer =";
cin>>n;
for(int i=0;i<n;i++)
{
cout<<"for costumer"<<i+1<<endl;
   cout<<"input name = ";
   cin>>a;
   cout<<"account number = ";
   cin>>b;
   cout<<"balance = ";
   cin>>c;

   info[i].getdata(a,b,c);
}

for(int i = 0 ; i<n;i++){
    info[i].display();
}

#include <iostream>
#include <string.h>
using namespace std;
class Time{
private:
int hr,min;

public:
void getdata(int a ,int b);
void display();
Time sum(Time, Time);
};


  void Time::getdata(int a ,int b){
 hr = a;
 min = b;
  display();
    }

  void Time::display(){
   cout<<"  hr =  "<<hr<<" min= "<<min<<endl;
}

Time Time::sum(Time t1, Time t2){
Time t3;
t3.hr=t1.hr+t2.hr;
t3.min=t1.min+t2.min;
if(t3.min>=60){
    t3.hr=t3.hr+t3.min/60;
    t3.min=t3.min%60;
    return t3;
}
else{
    return t3;
}
}


int main(){
Time T1,T2,T3;
T1.getdata(1,50);
T2.getdata(2,25);
T3=T3.sum(T1,T2);
T3.display();

}




}
