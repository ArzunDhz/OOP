#include <iostream>

using namespace std;

class toConvert{
private:
    int f ,c;
public:
  int getdata(int a){
      c=a;

  }
  int convert(){
  f=(c*9/5)+32;
  }
  int display(){
  cout<<"f="<<f;
  }

};
int main(){
toConvert c;
int a;
cout<<"c=";
cin>>a;
c.getdata(a);
c.convert();
c.display();
}

