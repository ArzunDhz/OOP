//outside the class

 void getdata(int a , char b[20] ,char c[20]);
int display();

class information{
private:
    int no;
    char name[10];
    char address[10];
public:

void getdata(int a , char b[20] ,char c[20]);
int display();

};



 void information::getdata(int a , char b[20] ,char c[20]){
      no = a;
      strcpy(name,b);
      strcpy(address,c);


  }

 int information::display(){
  cout<<"roll ="<<no<<"\n name="<<name<<" \n address"<<address<<endl;
  }


main(){
information info[20];
int n;
cout<<"no?";
cin>>n;
   int a;
char d[10];
char e[10];
for(int i = 0 ; i<n;i++){

    cout<<" for "<<i+1<<" roll =?  name=? address =?";
    cin>>a;
    cin>>d;
    cin>>e;
    info[i].getdata(a,d,e);


}

for(int i = 0 ; i<n;i++){
    info[i].display();
}
}


