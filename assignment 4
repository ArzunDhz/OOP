#include <iostream>
#include <iomanip>

using namespace std;
int display(int a ,int b);

int larger(int a , int b);

class greaterr{
private:
  int x,y;
public:

int larger(int a , int b);


};



 int greaterr::larger(int a , int b){
     x=a;
     y=b;

display(x,y);
  }

 int display(int a, int b ){
     if(a>b){
          cout<<"greater="<<a<<"\n smaller "<<b;
     }
     else cout<<"greater="<<b<<"\n smaller "<<a;

  }


main(){
    greaterr g;
    cout<<"enter 2 value";
    int a,b;
    cin>>a>>b;
    g.larger(a,b);
}


